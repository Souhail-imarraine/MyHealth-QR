{
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "src/server.js",
    "test/**/*.js",
    "**/*.test.js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "cache": false,
  "all": true
}
